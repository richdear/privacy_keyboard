<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
  <include latin:keyboardLayout="@xml/key_styles_common" />
  <Row
    latin:keyLabelFlags="fontNormal"
    latin:keyWidth="10%p">
    <include latin:keyboardLayout="@xml/rowkeys_bengali_bijoy1" />
  </Row>
  <Row
    latin:keyLabelFlags="fontNormal"
    latin:keyWidth="10%p">
    <include
      latin:keyboardLayout="@xml/rowkeys_bengali_bijoy2"
      latin:keyXPos="5%p" />
  </Row>
  <Row
    latin:keyLabelFlags="fontNormal"
    latin:keyWidth="10%p">

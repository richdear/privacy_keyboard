<resources>
  <!-- Locale display name exceptions.
       For each exception, there should be related string resources for display name that may have
       explicit keyboard layout. The string resource name must be "locale_name_<locale>". Please
       refer to strings.xml for these resources. -->
  <string-array name="locale_exception_keys">
    <item>en_US</item>
    <item>en_GB</item>
    <item>es_US</item>
    <item>hi_ZZ</item>
    <item>sr_ZZ</item>
  </string-array>

  <!-- Locale whose name should be displayed in Locale.ROOT. -->
  <string-array name="locale_displayed_in_root_locale">
    <item>hi_ZZ</item>
    <item>sr_ZZ</item>
  </string-array>

  <string name="locale_name_in_root_locale_hi_ZZ">Hinglish</string>
  <string name="locale_name_in_root_locale_sr_ZZ">Srpski</string>

  <!-- Predefined keyboard layouts for additional subtype -->
  <string-array name="predefined_layouts">
    <item>qwerty</item>
    <item>qwertz</item>
    <item>azerty</item>
    <item>dvorak</item>
    <item>colemak</item>
    <item>workman</item>
    <item>pcqwerty</item>
    <item>abc</item>
  </string-array>
  <!-- Predefined keyboard layout display names -->
  <string-array name="predefined_layout_display_names">
    <item>QWERTY</item>
    <item>QWERTZ</item>
    <item>AZERTY</item>
    <item>Dvorak</item>
    <item>Colemak</item>
    <item>Workman</item>
    <item>PC</item>
    <item>ABC</item>
  </string-array>
</resources>

<resources xmlns:tools="http://schemas.android.com/tools">
  <!-- For keyboard color scheme option dialog. -->
  <string-array name="keyboard_theme_names" translatable="false">
    <item>@string/keyboard_theme_pure_day</item>
    <item>@string/keyboard_theme_pure_night</item>
  </string-array>
  <!-- An element must be a keyboard theme id of
       {@link com.richdear.privacykeyboard.inputmethod.keyboard.KeyboardTheme#THEME_ID_ICS} etc. -->
  <integer-array name="keyboard_theme_ids" translatable="false" tools:ignore="InconsistentArrays">
    <item>6</item>
    <item>7</item>
  </integer-array>

  <array name="keyboard_theme_colors" translatable="false" tools:ignore="InconsistentArrays">
    <item>@color/background_lxx_pure_day</item>
    <item>@color/background_lxx_pure_night</item>
  </array>
</resources>
